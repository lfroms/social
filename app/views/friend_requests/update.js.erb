$("#friend-requests-list").html(`<%= render current_user.friend_requests %>`);
$("#activity").html(`<%= render "partials/recent_activity" %>`);
if ($("#friend-requests-list").is(':empty')) {
	$("#friend-request-container").fadeOut(500);
}

$("#user_listing_for_<%= @friend_request.friend_id %>").html(`<%= render "partials/friend_request_button", user: @friend_request.friend %>`);
